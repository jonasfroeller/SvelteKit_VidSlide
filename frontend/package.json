{
	"name": "vidslide",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"author": "Jonas Froeller",
	"homepage": "https://svelte-kit-vid-slide.vercel.app",
	"repository": {
		"type": "git",
		"url": "https://github.com/jonasfroeller/SvelteKit_VidSlide.git"
	},
	"description": "A TikTok, Instagram Reels and YouTube Reels inspired app. Post, edit and delete posts. Frontend hosted on Vercel. Backend hosted on OracleCloud. Built with SvelteKit, ViteJS, TailwindCSS, SkeletonUI, FloatingUI, Typesave-i18n, Iconify, Zod, JWT, MySQL, Apache, JS, TS, HTML, PHP, CSS.",
	"keywords": [
		"tiktok",
		"youtube-shorts",
		"instagram-reels",
		"social-media"
	],
	"license": "ISC",
	"main": "playwright.config.js",
	"directories": {
		"test": "tests"
	},
	"scripts": {
		"all": "npm-run-all --parallel dev typesafe-i18n",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"test": "playwright test",
		"test:unit": "vitest",
		"coverage": "vitest run --coverage",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"typesafe-i18n": "typesafe-i18n",
		"commit": "cz"
	},
	"devDependencies": {
		"@playwright/test": "^1.33.0",
		"@skeletonlabs/skeleton": "^1.5.1",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/adapter-node": "^1.2.4",
		"@sveltejs/kit": "^1.18.0",
		"@tailwindcss/forms": "^0.5.3",
		"@types/node": "^18.16.13",
		"autoprefixer": "^10.4.14",
		"commitizen": "^4.3.0",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.40.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"jsdom": "^22.0.0",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.23",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.0",
		"svelte": "^3.54.0",
		"svelte-check": "^3.3.2",
		"tailwindcss": "^3.3.2",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.5",
		"vite": "^4.3.8",
		"vitest": "^0.25.3"
	},
	"dependencies": {
		"@floating-ui/dom": "^1.2.8",
		"@sveltejs/adapter-vercel": "^2.4.3",
		"@tailwindcss/typography": "^0.5.9",
		"iconify-icon": "^1.0.7",
		"typesafe-i18n": "^5.24.3",
		"zod": "^3.21.4"
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	}
}